#nop;
#var aoe_enabled 0;

#alias {aoeon} {
    #var aoe_enabled 1;
    #show Armour of Elements corpse immolation enabled.;
    @addcorpsetrigger{0;aeoimmolate};
};
#alias {aoeoff} {
    #var aoe_enabled 0;
    #show Armour of Elements corpse immolate disabled.;
};



#alias aoeimmolate {
    immolate corpse;
};


