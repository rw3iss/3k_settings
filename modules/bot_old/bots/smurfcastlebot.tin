#class {bot} {open}
#var {areaname} {Smurf Castle}
#var {botpath} {w;e;e;w;s;w;e;e;w;s;w;e;e;w;s;w;e;e;w;s;w;e;e;w;s;w;e;s;w;e;e;w;n;e;out}
#act {-M-_%*{rat|spider|zreal|argamel}%*} 
 {
	#if {"%2" == "rat"} {#var mobname rat};
	#if {"%2" == "spider"} {#var mobname spider};
	#if {"%2" == "zreal"} {#var mobname azreal};
	#if {"%2" == "argamel"} {#var mobname gargamel};
	.check_monster
 } {1};
#class {bot} {close}
